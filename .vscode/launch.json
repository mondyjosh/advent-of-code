{
    "version": "0.2.0",
    "configurations": [        
    
        {
            "name": ".NET Core Launch (console)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceFolder}/src/csharp/AdventOfCode/bin/Debug/net6.0/AdventOfCode.dll",
            "args": ["2015", "2"],
            "cwd": "${workspaceFolder}/src/csharp",
            "console": "integratedTerminal",
            "stopAtEntry": false
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach"
        }
    ]
}
